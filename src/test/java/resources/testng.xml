<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="UdemyTests">
    <test name="Registration tests">
        <classes>
            <class name="com.udemy.RegistrationTest">
                <methods>
                    <include name="registrationWithValidData" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="Login with valid data">
        <classes>
            <class name="com.udemy.LoginTest">
                <methods>
                    <include name = "LoginWithValidData" />
                </methods>
            </class>
        </classes>
    </test>
</suite>